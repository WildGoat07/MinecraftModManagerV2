<UserControl x:Class="MinecraftModManagerV2.SelectProfilName"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:MinecraftModManagerV2"
             mc:Ignorable="d"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <ScrollViewer  Margin="0, 0, 0, 40">
            <StackPanel Name="pannel" />
        </ScrollViewer>
        <TextBox Name="inputName" TextWrapping="Wrap" VerticalAlignment="Bottom" Background="#FF323232" Foreground="White" SelectionBrush="#FF838383" FontFamily="Segoe UI Light" FontSize="14" Margin="10,0,117,10" />
        <Button Content="Enregistrer" Margin="0,0,10,10" Foreground="White" FontFamily="Segoe UI Light" FontSize="14" HorizontalAlignment="Right" Width="94" Height="21" VerticalAlignment="Bottom" Click="Button_Click" />
    </Grid>
</UserControl>